{
  "manifest_version": 3,
  "name": "Popup Animation Extension",
  "version": "3.4",
  "description": "A simple extension with an animated popup",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab"]
}
